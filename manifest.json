{
  "name": "Regex Password Generator",
  "short_name": "Regex PW Gen",
  "author": "John Kahn",
  "version": "1.0.0",
  "description": "Generates a password based on a regex string",
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/unlock-icon16.png",
      "32": "icons/unlock-icon32.png",
      "48": "icons/unlock-icon48.png",
      "128": "icons/unlock-icon128.png"
    }
  },
  "icons": {
    "16": "icons/lock-icon16.png",
    "32": "icons/lock-icon32.png",
    "48": "icons/lock-icon48.png",
    "128": "icons/lock-icon128.png"
  },
  "manifest_version": 2
}
